{
  "expo": {
    "name": "Mindful Map",
    "slug": "mindful-map",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "Used to deliver relevant wellness notifications."
      }
    },
    "android": {
      "package": "com.mindfulmap.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "permissions": [
        "NOTIFICATIONS"
      ],
      "notification": {
        "icon": "./assets/notification-icon.png",
        "color": "#4CAF50"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-notifications"
    ],
    "owner": "forborealis",
    "extra": {
      "eas": {
        "projectId": "eda71300-0a4e-4afa-8bf8-5390bcca4853"
      }
    }
  }
}
